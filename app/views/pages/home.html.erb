<div class="container">
  <h1 class="d-flex justify-content-center mt-3">Bienvenue au Gaec moutmout</h1>
  <p class="d-flex justify-content-center">Ensemble, comptons les moutons</p>
  <% @fields.each do |field| %>
    <%= button_to "#{field.name}", field_path(field), id: "#{field.id}", method: "get", class:"main-btns" %>
  <% end %>
</div>
